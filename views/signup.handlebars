<form class="card signupForm">
    <div>
        <label class="label" for="First Name">
            First Name
        </label>
        <input class="input" id="firstname" type="text">
    </div>
    <div>
        <label class="label" for="Last Name">
            Last Name
        </label>
        <input class="input" id="lastname" type="text">
    </div>
    <div>
        <label class="label" for="username">
            E-mail
        </label>
        <input class="input" id="email" type="text">
    </div>

    <div>
        <label class="label" for="password">
            Password
        </label>
        <input class="input" id="password" type="password">
    </div>

    <div class="loginFormFooter">
        <button class="button primaryButton" type="submit">Create Account</button>
    </div>
</form>

